<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}ET2020{% endblock %}</title>
    <script src="https://kit.fontawesome.com/d684afa6cf.js" crossorigin="anonymous"></script>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% include 'layout/_navbar.html.twig' %}


<div class="">
    {% for rubrique, messages in app.flashes %}
        <div class="alert alert-{{ rubrique }} text-center">
            {% for message in messages %}
                <p>{{ message }}</p>
            {% endfor %}
        </div>
    {% endfor %}
    {% block body %}{% endblock %}
</div>
</body>
</html>